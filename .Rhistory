library(tidyverse)
data <- data.frame(Group = c("Control","Experimental"),
Qpcr_mean = c(1,2),
Qpcr_SE = c(0.1,0.22))
ggplot(data,aes(x=Group,y=Qper_mean))+
geom_bar(stat="identity")
ggplot(data,aes(x=Group,y=Qpcr_mean))+
geom_bar(stat="identity")
ggplot(data,aes(x=Group,y=Qpcr_mean))+
geom_bar(stat="identity")+
labs(x="",
y="Relative Enrichment")+
theme_classic()
ggplot(data,aes(x=Group,y=Qpcr_mean))+
geom_bar(stat="identity")+
geom_errorbar(aes(x=Group,ymin=Qpcr_mean-Qpcr_SE,ymax=Qpcr_mean+Qpcr_SE))+
labs(x="",
y="Relative Enrichment")+
theme_classic()
ggplot(data,aes(x=Group,y=Qpcr_mean))+
geom_bar(stat="identity")+
geom_errorbar(aes(x=Group,ymin=Qpcr_mean-Qpcr_SE,ymax=Qpcr_mean+Qpcr_SE),width=0.25)+
labs(x="",
y="Relative Enrichment")+
theme_classic()
